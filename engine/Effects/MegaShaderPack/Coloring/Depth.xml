<effect>
<name>Depth</name>
<description>Change depth of color.</description>
<parameter>
		<name>Offset</name>
		<code>fOffset</code>
		<description>Offset of the source color, to adjust lighting (Coefficient/2 recommended)</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5 </value>
</parameter>
<parameter>
		<name>Coefficient</name>
		<code>fCoeff</code>
		<description>Divisor of color</description>
		<type>float</type>
		<property>edit</property>
		<value>1</value>
		<min>0</min>
</parameter>
</effect>

