<effect>
<name>Blur Real</name>
<description>Real blur</description>
<author>Sphax - Flavien Clermont</author>
<company>Complex-softwares</company>
<website>http://complex.softwares.free.fr</website>
<parameter>
		<name>Coeff</name>
		<code>fCoeff</code>
		<description>Blur offset coefficient for one step</description>
		<type>float</type>
		<property>edit</property>
		<value>0.01</value>
		<min>0.0</min>
		<max>0.5</max>
</parameter>
<parameter>
		<name>Steps</name>
		<code>nSteps</code>
		<description>Number of steps</description>
		<type>int</type>
		<property>edit</property>
		<value>1</value>
		<min>0</min>
</parameter>
</effect>

