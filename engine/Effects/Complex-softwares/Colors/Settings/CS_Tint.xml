<effect>
<name>Tint</name>
<description>Apply a tint to the object</description>
<author>Sphax - Flavien Clermont</author>
<company>Complex-softwares</company>
<website>http://complex.softwares.free.fr</website>
<parameter>
		<name>Tint color</name>
		<code>fTintColor</code>
		<description>Tint color</description>
		<type>int_float4</type>
		<property>COLOR</property>
		<value>0</value>
</parameter>
<parameter>
		<name>Tint power</name>
		<code>fTintPower</code>
		<description>Tint power</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5</value>
</parameter>
<parameter>
		<name>Original power</name>
		<code>fOriginalPower</code>
		<description>Original power</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
</parameter>
</effect>

