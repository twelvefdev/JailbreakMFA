</parameter>
<parameter>
		<name>Height</name>
		<code>fHeight</code>
		<description>Height coefficient</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
</parameter>
</effect>

