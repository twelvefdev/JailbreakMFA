<effect>
<name>Alpha Clamp</name>
<description>Clamps the alpha channel.</description>
<author>NaitorStudios</author>
<company></company>
<website></website>
<parameter>
		<name>Threshold</name>
		<code>threshold</code>
		<description></description>
		<type>float</type>
		<property>spin</property>
		<value>0.5</value>
		<min>0.0</min>
		<max>1.0</max>
</parameter>

<parameter>
		<name>Lower Clamp</name>
		<code>lowerclamp</code>
		<description></description>
		<type>float</type>
		<property>spin</property>
		<value>0.0</value>
		<min>0.0</min>
		<max>1.0</max>
</parameter>

<parameter>
		<name>Upper Clamp</name>
		<code>upperclamp</code>
		<description></description>
		<type>float</type>
		<property>spin</property>
		<value>1.0</value>
		<min>0.0</min>
		<max>1.0</max>
</parameter>

<parameter>
		<name>Opacity</name>
		<code>opacity</code>
		<description>Since blend coef should affect the result, this parameter was added to replace it.</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
		<max>1.0</max>
</parameter>

</effect>

