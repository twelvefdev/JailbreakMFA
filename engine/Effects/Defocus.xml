<effect>
<name>Defocus</name>
<author>Pixelthief</author>
<description>Guassian Blurs, Desaturates and Darkens the background</description>
<BackgroundTexture>1</BackgroundTexture>
<parameter>
		<name>Blur Coefficient</name>
		<code>fCoeff</code>
		<description>Blurs by this %% of the texture's width/height</description>
		<type>float</type>
		<property>edit</property>
		<value>0.005</value>
		<min>0.0</min>
		<max>1.0</max>
</parameter>
<parameter>
		<name>Blackening Coefficient</name>
		<code>fBlack</code>
		<description>Darkens by this %% towards black</description>
		<type>float</type>
		<property>edit</property>
		<value>0.25</value>
		<min>0.0</min>
		<max>1.0</max>
</parameter>
<parameter>
		<name>Desaturation Coefficient</name>
		<code>fDesat</code>
		<description>Desaturates colors by this %%</description>
		<type>float</type>
		<property>edit</property>
		<value>0.75</value>
		<min>0.0</min>
		<max>1.0</max>
</parameter>
</effect>
